package fact;
import java.util.Scanner;
public class fact {

			int facto(int n) {
				int fct=1;
				for(int i=0;i<n;i++)
				{
				 fct=fct*n;
				 n=n-1;
				}
				return fct;
			}
			public static void main(String[] args) { 
			fact fs=new fact();
			
			Scanner sc=new Scanner(System.in);
			System.out.print("Enter the value of n:");
			int s=sc.nextInt();
			int factorial=fs.facto(s);
			System.out.println(factorial);
			
		}
	}

